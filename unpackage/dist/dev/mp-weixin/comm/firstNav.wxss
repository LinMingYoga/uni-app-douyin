
.lm-nav {
	width: 100%;
	position: fixed;
	top: 25px;
	left: 0;
	height: 35px;
	line-height: 35px;
	background-color: rgba(0,0,0,.5);
	color: #FFFFFF;
	z-index: 999;
}
.lm-nav-lt {
	width: 20%;
	position: absolute;
	left: 0px;
	font-size: 20px;
	text-align: center;
}
.lm-nav-ct {
	width: 100px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin: 0 auto;
}

